{
  "name": "@yukiakai/find-up",
  "version": "1.1.5",
  "description": "A flexible find-up implementation supporting string, RegExp, CJS/ESM dual build, and custom matcher.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "test": "npm run build && npx vitest run --reporter verbose --coverage",
    "format": "prettier --write **/*.{js,ts}",
    "format:check": "prettier --check **/*.{js,ts}",
    "lint": "eslint src/**/*.ts",
    "build": "npx tsup",
    "docs": "npx typedoc"
  },
  "files": [
    "dist/*",
    "CHANGELOG.md"
  ],
  "keywords": [
    "find-up",
    "find up",
    "resolve",
    "file",
    "folder",
    "regex",
    "cjs",
    "esm",
    "typescript",
    "matcher"
  ],
  "author": "Yuki_akai",
  "homepage": "https://github.com/yukiakai212/find-up",
  "engines": {
    "node": ">= 16"
  },
  "maintainers": [
    {
      "name": "Yuki_akai",
      "homepage": "https://github.com/yukiakai212"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukiakai212/find-up.git"
  },
  "bugs": {
    "url": "https://github.com/yukiakai212/find-up/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.10",
    "@vitest/coverage-v8": "^4.0.15",
    "dotenv": "^17.0.1",
    "eslint": "^9.30.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.9",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^4.0.15"
  }
}
